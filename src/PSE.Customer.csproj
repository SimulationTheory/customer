        <PackageReference Include="PSE.WebAPI.Core" Version="0.0.40" />
    </ItemGroup>

    <ItemGroup>
        <DotNetCliToolReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Tools" Version="2.0.0" />
    </ItemGroup>

    <ItemGroup>
      <Content Update="localConfiguration.json">
